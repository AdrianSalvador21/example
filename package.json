{
  "name": "sabadell-console",
  "version": "6.1.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --open --proxy-config proxy.conf.json",
    "http-server": "http-server dist/ -c-1",
    "build": "ng build",
    "build:prod": "ng build --prod --configuration=prod --base-href \"/sabadell_console_ui/\"",
    "build:dev": "ng build --prod --configuration=dev --base-href \"/sabadell_console_ui/\"",
    "build:accept": "ng build --prod --configuration=accept --base-href \"/sabadell_console_ui/\"",
    "build:pre": "ng build --prod --configuration=pre --base-href \"/sabadell_console_ui/\"",
    "build:library": "ng build ngx-example-library",
    "deploy": "ng build --prod && npx ngh",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ci": "npm run lint && npm run test && npm run e2e",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/*.json --sort --format namespaced-json --format-indentation '  ' --marker _",
    "translate": "node ./scripts/translate.js",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t angularexampleapp . && docker run -d -p 4200:80 angularexampleapp",
    "update": "ng update --all --force"
  },
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "@babel/polyfill": "^7.2.5",
    "@ncstate/sat-popover": "3.0.0",
    "angular-user-idle": "^2.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.7",
    "element-remove": "^1.0.4",
    "grunt": "1.5.2",
    "grunt-war": "0.5.1",
    "guid-typescript": "^1.0.9",
    "mime-types": "^2.1.22",
    "ng2-tooltip-directive": "2.1.5",
    "ngx-currency": "^1.5.1",
    "ngx-mask": "^7.0.1",
    "ngx-scrollbar": "3.2.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "sass-loader": "^6.0.7",
    "tippy.js": "3.2.0",
    "tooltip.js": "1.3.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular-devkit/build-ng-packagr": "0.11.0",
    "@angular/cdk": "^7.1.1",
    "@angular/cli": "7.1.0",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "7.1.1",
    "@angular/material": "7.1.1",
    "@angular/platform-server": "7.1.1",
    "@angular/service-worker": "7.1.1",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "3.3.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.10",
    "angular-cli-ghpages": "0.5.3",
    "axios": "0.18.0",
    "bowser": "2.0.0-beta.3",
    "codelyzer": "4.5.0",
    "coveralls": "3.0.2",
    "deepmerge": "2.2.1",
    "fs": "0.0.1-security",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-alpha.12",
    "http-server": "0.11.1",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "3.0.0",
    "moment": "2.22.2",
    "ng-packagr": "^4.7.1",
    "ngx-store": "2.1.0",
    "path": "0.12.7",
    "protractor": "5.4.1",
    "standard-version": "4.4.0",
    "ts-helpers": "1.1.2",
    "ts-node": "7.0.1",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.1.1",
    "webpack-bundle-analyzer": "3.0.3"
  }
}
